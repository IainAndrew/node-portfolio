{
  "name": "node-portfolio",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "watch": "npm-run-all --parallel watch:server watch:build",
    "watch:build": "webpack --env=dev --watch",
    "watch:server": "nodemon ./bin/www",
    "build:dev": "webpack --env=dev",
    "build:prod": "webpack --env=prod"
  },
  "dependencies": {
    "body-parser": "^1.20.2",
    "contentful": "^10.6.15",
    "cookie-parser": "^1.4.6",
    "express": "^4.18.2",
    "morgan": "^1.10.0",
    "pug": "^3.0.2",
    "serve-favicon": "^2.5.0"
  },
  "nodemonConfig": {
    "ignore": [
      "public/**/*"
    ]
  },
  "devDependencies": {
    "autoprefixer": "^10.4.16",
    "babel-core": "^6.26.3",
    "babel-loader": "^9.1.3",
    "babel-preset-env": "^1.7.0",
    "compression-webpack-plugin": "^10.0.0",
    "css-loader": "^6.8.1",
    "foundation-sites": "^6.8.1",
    "jquery": "^3.7.1",
    "lazysizes": "^5.3.2",
    "mini-css-extract-plugin": "^2.7.6",
    "nodemon": "^3.0.2",
    "npm-run-all": "^4.1.5",
    "postcss-loader": "^7.3.4",
    "sass": "^1.69.7",
    "sass-loader": "^13.3.3",
    "style-loader": "^3.3.3",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4"
  }
}
